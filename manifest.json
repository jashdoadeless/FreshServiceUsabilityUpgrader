{
  "name": "FreshService Usability Upgrader",
  "version": "0.0.1",
  "description": "This extension provides extra functionality to the freshservice support pages.",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.freshservice.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}